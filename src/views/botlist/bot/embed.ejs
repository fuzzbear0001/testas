
<%- include('../../parts/links') %>
<title><%= botdata.username %> | Discord Bots</title>
<head>
    
    <link rel="icon" href="<%= botdata.avatar %>" type="image/x-icon" />
    <meta property="og:title" content="<%= botdata.username %>">
    <meta property="og:description" content="<%= botdata.shortDesc %>">
    <meta property="og:image" content="<%= botdata.avatar %>">
</head>


    

    <style>
        .separator {
  display: flex;
  align-items: center;
  text-align: center;
}

.separator::after {
  content: '';
  flex: 2;
  border-bottom: 1px solid #fff;
}

.separator:not(:empty)::before {
  margin-right: .55em;
}

.separator:not(:empty)::after {
  margin-left: .55em;
}

  </style>

    <!-- Page content -->
    <div style = "position:relative;left:3px;top:1px;">
        <div class="col-xl-4 order-xl-2 mb-5 mb-xl-0">
          <div class="card card-profile">
            
            <div class="card-header text-center border-0 pt-8 pt-md-4 pb-0 pb-md-4">
              <div class="d-flex justify-content-between">
                <a href="/bot/<%= botdata.botID%>/vote"  target="_blank" class="btn btn-vcodes mr-4"><i class="fad fa-vote-yea"></i> Votes: <%= botdata.votes || "0" %> </a>
                <%= botdata.username%>
            <a href="/bot/<%= botdata.botID%>/invite"  target="_blank" class="btn btn-vcodes float-right"><i class="fad fa-link"></i> Invite</a>
            
              </div><br><br>
              <div class="row justify-content-center">
              <div class="col-lg-3 order-lg-2">
                <div class="card-profile-image">
                  <span class="avatar">
                    <img alt="Image placeholder" onerror="imgError(this)" class="rounded-circle mr-4" src="<%= botdata.avatar %>">
                  </span>
                </div>
              </div>
            </div>

              <div class="text-center">
                <h3 translate="no">
                  <%= botdata.username %> 
                  <% if(botdata.botID === "746673633551974456") {%>
                    <a data-tooltip="Server Bot"
                    data-tooltip-location="top"><i class="fad fa-server bodyIcon"></i></a>
                 <% } %>
                 
                </h3>
                <div>
              <div>
              <div class="d-flex justify-content-between">
                <a href="https://bhbotlist.xyz"  target="_blank" style="background: rgb(33, 167, 111);" class="btn btn-vcodes mr-4"><i class="fad fa-vote-yea">&nbsp;&nbsp;</i>B.H Botlist</a>
                
                
                
            -            <a href="<%= botdata.website %>" style="background:"  class="btn btn-vcodes mr-4"><i class="mt-1 fad fa-circle status-<%= bot.users.cache.get(botdata.botID).presence.status %>"></i> Status</a>	
              </div>
              </div>
            </div>
            </div>
            
         

