<%- include('../parts/head', {bot, user, path}) %>
<script async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-1831947467874577"
     crossorigin="anonymous"></script>

<title><%= member.username %> | Profile</title>
<head>
    <meta name="og:title" content="<%= member.username %> | Profile">
    <meta name="og:description" content="<%= pdata ? (pdata.biography ? pdata.biography : member.username+' doesn\'t have a biography.') : member.username+' doesn\'t have a biography.'%>">
    <meta name="og:image" content="https://cdn.discordapp.com/avatars/<%=member.id%>/<%=member.avatar%>.png">
</head>
<div class="container px-3 px-3 mt-5 pt-5">
    <div class="columns container px-3 is-multiline">
        <div class="column is-2">
            <img onerror="imgError(this);" draggable="false" class="pulse-avatar-<%=member.presence.status%>"  onerror=" imgerror(this);"="" src="<%= member.avatarURL({dynamic: true}) %>" style="width: 300px; border-radius: 50%;">
        </div>
        <div class="column is-6">
          <p translate="no" class="has-text-white is-size-3" style="margin-bottom: 10px;"><%= member.username %><span class="is-size-4" style="opacity: 0.7;">#<%= member.discriminator %></span>&nbsp;<i class="status-circle fad fa-circle status-<%=member.presence.status%>"></i></p>
           <div class="user-badges">
            <% 
            let cacheServer = bot.guilds.cache.get(config.server.id).members.cache.get(member.id) 
            %>
          <% if(cacheServer) {%>
            <%
            let webRole = cacheServer.roles.cache.get(roles.administrator);
            let adminRole = cacheServer.roles.cache.get(roles.administrator);
            let moderatorRole = cacheServer.roles.cache.get(roles.moderator);
            let sponsorRole = cacheServer.roles.cache.get(roles.profile.sponsor);
            let premiumDeveloper = cacheServer.roles.cache.get(roles.botlist.premium_developer);
            let certifiedDeveloper = cacheServer.roles.cache.get(roles.botlist.certified_developer);
            let Developer = cacheServer.roles.cache.get(roles.botlist.developer);
            let Bughunt = cacheServer.roles.cache.get(roles.botlist.bug);
            let Staff = cacheServer.roles.cache.get(roles.botlist.staff);
            let partnerRoleCache = cacheServer.roles.cache.get(roles.profile.partnerRole);
            let supporterRole = cacheServer.roles.cache.get(roles.profile.supporter);
            %>
            <% if (adminRole) { %>
            <span data-tooltip="Website Developer" data-tooltip-location="top"> <img draggable="false" src="https://media.discordapp.net/attachments/1037009360091619378/1038610076820316160/website_dev.png?width=670&height=670" width="32px"></span>
            <% } %>
            <% if (adminRole) { %>
            <span data-tooltip="Creator Calypso" data-tooltip-location="top"> <img draggable="false" src="https://cdn.discordapp.com/attachments/1037009360091619378/1038612523093610496/owner.png" width="32px"></span>
            <% } %>
            <% if (moderatorRole) { %>
            <span data-tooltip="Moderator Calypso" data-tooltip-location="top"> <img draggable="false" src="https://cdn.discordapp.com/attachments/1037009360091619378/1038611168849313852/moderator.png" width="32px"></span>
            <% } %>
            <% if (Staff) { %>
            <span data-tooltip="Team Calypso" data-tooltip-location="top"> <img draggable="false" src="https://cdn.discordapp.com/attachments/1037009360091619378/1038613220136599603/team.png" width="32px"></span>
            <% } %>
            <% if (Bughunt) { %>
            <span data-tooltip="Bug Hunter" data-tooltip-location="top"> <img draggable="false" src="https://cdn.discordapp.com/attachments/1037009360091619378/1038613438898896956/bughunter.png" width="32px"></span>
            <% } %>
            <% if (sponsorRole) { %>
            <span data-tooltip= "Sponsor"" data-tooltip-location="top"> <img draggable="false" src="https://cdn.discordapp.com/attachments/1037009360091619378/1038616242019381318/sponsor.png" width="32px"></span>
            <% } %>
            <% if (premiumDeveloper) { %>
            <span data-tooltip="Premium" data-tooltip-location="top"> <img draggable="false" src="https://cdn.discordapp.com/attachments/1037009360091619378/1038621085752643604/premium.png" width="32px"></span>
            <% } %>
            <% if (certifiedDeveloper) { %>
            <span data-tooltip="Developper Certified" data-tooltip-location="top"> <img draggable="false" src="https://cdn.discordapp.com/attachments/1037009360091619378/1038622548935581696/devcertif.png" width="32px"></span>
            <% } %>
            <% if (Developer) { %>
            <span data-tooltip="Developper" data-tooltip-location="top"> <img draggable="false" src="https://cdn.discordapp.com/attachments/1037009360091619378/1038622416336867348/dev.png" width="32px"></span>
            <% } %>
            <% if (partnerRoleCache) { %>
            <span data-tooltip="<%=partnerRoleCache.name%>" data-tooltip-location="top"> <span data-tooltip="Calypso Partner" data-tooltip-location="top"><img draggable="false" src="https://media.discordapp.net/attachments/1037009360091619378/1038606760568705044/my-icon.png?width=670&height=670" width="32px"></span></span>
            <% } %>
            <% if (supporterRole) { %>
            <span data-tooltip="Supporter" data-tooltip-location="top"> <img draggable="false" src="https://cdn.discordapp.com/attachments/1037009360091619378/1038623150755287090/supporter.png" width="32px"></span>
            <% } %>
            <% } %>
            <% if(member.flags) {%>
            <% member.flags.toArray().forEach(flag =>{
            if (flag === "HOUSE_BALANCE") { %>
            <span data-tooltip="Hypesquad Balance" data-tooltip-location="top"><img draggable="false" src="https://discords.com/bio/img/hs_balance.9fdc63ef.svg" data-tooltip="Hypesquad Balance" data-tippy-content="Hypesquad Balance" width="32px"></span>
            <% } if (flag === "HOUSE_BRILLIANCE") { %>
            <span data-tooltip="Hypesquad Brilliance"data-tooltip-location="top"><img draggable="false" src="https://cdn.discordapp.com/attachments/1037370308065251409/1038603678069817445/48cf0556d93901c8cb16317be2436523.png" data-tippy-content="HypeSquad Brilliance" width="32px"></span>
            <% } if (flag === "HOUSE_BRAVERY") { %>
            <span data-tooltip="Hypesquad Bravery"data-tooltip-location="top"><img draggable="false" src="https://discords.com/bio/img/hs_bravery.1115767a.svg" data-tippy-content="HypeSquad Bravery" width="32px"></span>
            <% } if (flag === "VERIFIED_DEVELOPER") { %>
            <span data-tooltip="Verified Developer"data-tooltip-location="top"><img draggable="false" src="https://discords.com/bio/img/verified_developer.45cd06af.svg" data-tippy-content="Early Verified Bot Developer" width="32px"></span>
            <% } if (flag === "EARLY_SUPPORTER") { %>
            <span data-tooltip="Early Supporter" data-tooltip-location="top"><img draggable="false" src="https://discords.com/bio/img/early_supporter.23e59d79.svg" data-tippy-content="Early Supporter" width="32px"></span>
            <% } }) %>
            <% } %> 
            <% if(bannedCheck) { %>
            &nbsp;<span data-tooltip="Banned from BHBotlist" data-tooltip-location="top"> <i class="fad fa-ban has-text-danger" style="color: RED;bottom: -5px; font-size: 26px;"></i></span>
            <% } %>
            </div>             
            <% if(pdata) {%>
            <% if(pdata.biography) { %>
            <p translate="no" class="mt-1 has-text-white"><%=pdata.biography%></p>
            <% } else { %>
            <p class="mt-1 has-text-white"><%=member.username%> doesn't have a biography.</p>
            <% } %>
            <% } else { %>
            <p class="mt-1 has-text-white"> <%= member.username %> doesn't have a biography.</p>
            <% } %>
	    <%if(req.user) {%>
            <% if(req.user.id === member.id) {%>
                <a class="btn btn-vcodes" href="/user/<%=member.id%>/edit"><i class="fad fa-pencil"></i> Edit Profile</a>
                <% } %>
		<% } %>
        </div>
        <div class="is-4 mt-1 column">
            <% if(pdata) {%>
                <% if(pdata.twitter) {%>
                    <a href="https://twitter.com/<%= pdata.twitter %>" class="is-flex mt-1">
                      <div style="background: #2077aa" class="box-2-p"><i class="fab fa-twitter is-size-6 mt-1"></i></div>
                      <p style="background: #019ffa" class="box-2">Twitter</p>
                    </a>
                <% } %>
                <% if(pdata.instagram) {%>
                <a href="https://instagram.com/<%= pdata.instagram %>" class="is-flex mt-1">
                    <div style="background: #923453" class="box-2-p"><i class="fab fa-instagram is-size-6 mt-1"></i></div>
                    <p style="background: #E1306C;color: #fff!important" class="box-2">Instagram</p>
                </a>
                 <% } %>
                  <% if(pdata.website) {%>
                  <a href="<%= pdata.website %>" class="is-flex mt-1">
                    <div style="background: rgb(33, 167, 111)" class="box-2-p"><i class="fa fa-globe is-size-6 mt-1"></i></div>
                    <p style="background: rgb(8, 187, 112);color: #fff!important"class="box-2">Website</p>
                  </a>
                  <% } %>
                  <% if(pdata.github) {%>
                  <a href="https://github.com/<%= pdata.github %>" class="is-flex mt-1">
                    <div style="background: rgb(0, 0, 0);" class="box-2-p"><i class="fab fa-github is-size-6 mt-1"></i></div>
                    <p style="background: rgb(8, 8, 8);color: #fff!important" class="box-2">Github</p>
                  </a>
                  <% } %>
            <% } %>
        </div>
            <br>
    <div class="column mt-5">
    <p class="has-text-white title is-large"><i class="fa fa-robot"></i>&nbsp;<%= member.username%>'s Bots</p>
    <p class="content has-text-white" style="margin-top: -20px;">They are ranked according to the votes they received.</p>
    <% if(botdata.filter(a => (member.id === a.ownerID || a.coowners.includes(member.id))).length <= 0) { %>
      <div class="alert alert-danger" role="alert">
       <span style="color: #fff!important;" class="is-size-5"><i class="fad fa-robot"></i>&nbsp;<strong>Oops..</strong></span><br>
       <strong><%=member.username%></strong> does not have any bots in the system.
      </div>
      <% } %>
    <div class="row">
      <% botdata.filter(a => (member.id === a.ownerID || a.coowners.includes(member.id))).sort((a, b) => b.votes - a.votes).map(a => {%>
      <% if(a.status == "Approved") { %>
        <div class="col-md-6 col-lg-4 pb-3">
          <div class="card vcodes-card-custom bg-vcodes border-white border-0">
            <div class="vcodes-card-custom-img" style="background-image: url(<%= a.backURL || "../../img/undefined_back.png" %>);"></div>
            <div class="vcodes-card-custom-avatar">
              <img class="img-fluid" onerror="imgError(this);" src="<%=a.avatar%>" alt="Avatar" />
            </div>
            <div class="card-body">
             <div class="is-pulled-right mt-vcodes mt-1">
                  <span data-tooltip="Votes this Month" data-tooltip-location="left" class="badge badge-stats mb-vcodes mt-1 w-100"><i class="fad fa-chevron-up"></i>&nbsp;<%= a.votes || "0" %></span>
                  <span data-tooltip="Server Count" data-tooltip-location="left" class="badge badge-stats mt-1 w-100"><i class="fad fa-server"></i>&nbsp;<%= a.serverCount || "N/A" %></span>
                </div>
              <h4 translate="no" class="card-title"><%= a.username %>
              <% if(a.certificate === "Certified") { %>
                <span data-tooltip="Verified Bot" data-tooltip-location="top"><i class="fad fa-badge-check has-text-success"></i></span>
              <% } %>
              </h4>
              <p translate="no" class="card-text vCodesbotDesc"><%= a.shortDesc %></p>
            </div>
            <div class="card-footer">
              <a href="/bot/<%=a.botID%>" class="btn  btn-bot">View</a>
              <a href="https://discord.com/oauth2/authorize?client_id=<%=a.botID%>&scope=bot&permissions=8" class="btn  btn-bot">Invite</a>
            </div>
          </div>
        </div>
      <% } else { %>
      <% 
      if(req.user) {
      if(req.user.id === a.ownerID || a.coowners.includes(req.user.id)) { 
      %>
        <div class="col-md-6 col-lg-4 pb-3">
          <div class="card vcodes-card-custom bg-vcodes border-white border-0">
            <div class="vcodes-card-custom-img" style="background-image: url(<%= a.backURL || "../../img/undefined_back.png" %>);"></div>
            <div class="vcodes-card-custom-avatar">
              <img class="img-fluid" onerror="imgError(this);" src="<%=a.avatar%>" alt="Avatar" />
            </div>
            <div class="card-body">
             <div class="is-pulled-right mt-vcodes mt-1">
                  <span data-tooltip="Votes this Month" data-tooltip-location="left" class="badge badge-stats mb-vcodes mt-1 w-100"><i class="fad fa-chevron-up"></i>&nbsp;<%= a.votes || "0" %></span>
                  <span data-tooltip="Server Count" data-tooltip-location="left" class="badge badge-stats mt-1 w-100"><i class="fad fa-server"></i>&nbsp;<%= a.serverCount || "N/A" %></span>
                </div>
              <h4 translate="no" class="card-title"><%= a.username %>
              <% if(a.certificate === "Certified") { %>
                <span data-tooltip="Verified Bot" data-tooltip-location="top"><i class="fad fa-badge-check has-text-success"></i></span>
              <% } %>
              </h4>
              <p translate="no" class="card-text vCodesbotDesc"><%= a.shortDesc %></p>
            </div>
            <div class="card-footer">
              <a href="/bot/<%=a.botID%>" class="btn  btn-bot">View</a>
              <a href="https://discord.com/oauth2/authorize?client_id=<%=a.botID%>&scope=bot&permissions=8" class="btn  btn-bot">Invite</a>
            </div>
          </div>
        </div>
      <% 
      }
      }
      } 
      %>
      <% }) %>

  </div>
      <p class="has-text-white title is-large mt-5"><i class="fad fa-server"></i>&nbsp;<%= member.username %>'s Servers</p>
    <p class="content has-text-white" style="margin-top: -20px;">They are ranked according to the votes they received.</p>
        <% if(serverData.filter(a => global.clientSL.guilds.cache.get(a.id) && global.clientSL.guilds.cache.get(a.id).members.cache.get(member.id) && global.clientSL.guilds.cache.get(a.id).members.cache.get(member.id).permissions.has('ADMINISTRATOR') === true).length < 1) { %>
      <div class="alert alert-danger" role="alert">
       <span style="color: #fff!important;" class="is-size-5"><i class="fad fa-robot"></i>&nbsp;<strong>Oops..</strong></span><br>
       <strong><%=member.username%></strong> does not have any servers in the system.
      </div>
    <% } %>
    <div class="columns is-multiline is-desktop is-variable is-7 pt-5">

     <% serverData.filter(a => global.clientSL.guilds.cache.get(a.id) && global.clientSL.guilds.cache.get(a.id).members.cache.get(member.id) && global.clientSL.guilds.cache.get(a.id).members.cache.get(member.id).permissions.has('ADMINISTRATOR') === true).sort((a, b) => b.votes - a.votes).map(a => { %>
<% let guildCache = global.clientSL.guilds.cache.get(a.id) %>
      <div class="col-md-6 col-lg-4 pb-3">
        <div data-aos="fade-left">
        <div class="card vcodes-card-custom bg-vcodes border-white border-0">
          <div class="vcodes-card-custom-img" onerror="backImgError(this);" style="background-image: url(<%= guildCache.banner ? 'https://cdn.discordapp.com/banners/'+a.id+'/'+guildCache.banner+'?size=1024' : 'https://bhbotlist.xyz/img/undefined_back.png' %>);" title="BHBotlist.xyz" ></div>
          <div class="vcodes-card-custom-avatar">
            <img class="img-fluid" title="Discord Bot Avatar" onerror="imgError(this);" src="<%=a.icon%>" alt="Avatar" />
          </div>
          <div class="card-body">
           <div class="is-pulled-right mt-vcodes mt-1" style="max-width: 28.5%!important;">
            <span data-tooltip="Votes this Month" data-tooltip-location="left" class="badge badge-stats mb-vcodes mt-1 w-100"><i class="fad fa-chevron-up"></i>&nbsp;<%= a.votes %></span><span data-tooltip="Bumps this Month" data-tooltip-location="left" class="badge badge-stats mb-vcodes mt-1 w-100"><i class="fad fa-bomb"></i>&nbsp;<%= a.bumps %></span>
            <span data-tooltip="Members" data-tooltip-location="left" class="badge badge-stats mt-1 w-100"><i class="fad fa-users"></i>&nbsp;<%= guildCache.memberCount %></span>
              </div>
            <p translate="no" class="card-title is-size-4"><%= a.name %>
            <% if(guildCache.partnered === true) { %>
              <span data-tooltip="Partnered Discord" data-tooltip-location="top">
              <i class="fad fa-handshake bodyIcon"></i></span>
            <% } %>
            </p>
            <p translate="no" class="card-text vCodesbotDesc"><%= a.shortDesc %></p>
          </div>
          <div class="card-footer">
            <a title="bhbotlist" href="/server/<%=a.id%>" class="btn  btn-bot">View</a>
            <a title="bhbotlist" href="/server/<%=a.id%>/join" class="btn  btn-bot">Join</a>
          </div>
        </div>
      </div>
      </div>
      <% }) %>
    </div>
</div>    
</div>
</div>
</div>
<script>
var urlParams = new URLSearchParams(window.location.search);
    if(urlParams.has('success') && urlParams.get('success')){
    Swal.fire({
     icon: 'success',
     title: 'Successful...',
     closeOnConfirm: false,
     text: urlParams.get('message'),
     footer: 'Copyright 2021 bhbotlist.xyz - All right reserved.'
    }).then((result) => {
  if (result.isConfirmed) {
    window.location.href = `/user/<%= req.params.userID %>`;
  }
})
}
</script>
<%- include('../parts/foot') %>



